<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.tylz.mmsapling"
          xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:name=".base.BaseApplication"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.TextInputLayoutUI"/>
        <activity android:name=".ui.AppBarLayoutUI"/>
        <activity android:name=".ui.TabLayoutUI"/>
        <activity android:name=".ui.example.LoadingPagerUI"/>
        <activity android:name=".ui.example.LoadingPagerPowerUI"/>
        <activity android:name=".ui.example.SuperAdapterUI"/>
        <activity android:name=".ui.example.HolderUI"/>
    </application>

</manifest>